<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="150.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">

        <GridPane layoutX="75.0" layoutY="15.0">
            <Text text="Name of Obstacle" GridPane.rowIndex="0" />
            <!--Reads obstacle name-->
            <TextField fx:id="obstacleName" GridPane.rowIndex="1" />
            <Text text="Height (m)" GridPane.rowIndex="2" />
            <!--Reads obstacle height-->
            <TextField fx:id="obstacleHeight" GridPane.rowIndex="3" />
      <columnConstraints>
         <ColumnConstraints />
      </columnConstraints>
      <rowConstraints>
         <RowConstraints />
         <RowConstraints />
         <RowConstraints />
         <RowConstraints />
      </rowConstraints>
        </GridPane>
        <!--When pressed, the button calls obstacle definition function in Controller-->
        <Button fx:id="obstacleDoneButton" layoutX="100.0" layoutY="110.0" onAction="#defineObstacle" text="Add Obstacle" textAlignment="CENTER">
      <font>
         <Font name="System Bold" size="13.0" />
      </font></Button>




</AnchorPane>
