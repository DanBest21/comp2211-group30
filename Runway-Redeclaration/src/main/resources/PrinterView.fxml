<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<GridPane xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          prefHeight="400.0" prefWidth="600.0">
    <Text text="Choose view to print"  GridPane.rowIndex="1" GridPane.columnIndex="1"/>
    <ComboBox fx:id="printBox" GridPane.rowIndex="2" GridPane.columnIndex="1" promptText="Choose view"/>
    <Button onAction="#print" GridPane.rowIndex="3" GridPane.columnIndex="1" text="Print"/>

</GridPane>
